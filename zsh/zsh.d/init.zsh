#!/usr/bin/env zsh
# Load all submodules

DIR="$HOME/.zsh.d/"

source "$DIR/setopt.zsh"
source "$DIR/autoload.zsh"
source "$DIR/bindings.zsh"
source "$DIR/prompt.zsh"
source "$DIR/env.zsh"
source "$DIR/aliases.zsh"
source "$DIR/git.zsh"
source "$DIR/fzf.zsh"
[ -f "$DIR/catawiki.zsh" ] && source "$DIR/catawiki.zsh"
